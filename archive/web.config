<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Redirect /archive/downloads.php" stopProcessing="true">
                    <match url="^downloads.php" />
                    <action type="Redirect" url="/downloads/archive/" redirectType="Permanent" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
